<%= render 'books/list', user: current_user, book: @book %>

<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
    <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th colspan-"3"></th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td class="user_image" ><%= image_tag user.get_profile_image(80,80) %></td>
          <td class="user_name align-top"><%= user.name %></td>
          <td class="btn"><%= link_to "Show", user_path(user.id) %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
</div>